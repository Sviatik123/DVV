@using SubChoice.Core.Data.Entities
@{
    ViewData["Title"] = "Teachers To Approve";
}
<div class="d-flex justify-content-between my-4"><h1 class="p-0">@ViewData["Title"]</h1> <a asp-controller="Home" asp-action="Create" class="btn btn-login">New</a></div>

<table class="table">
    <thead>
        <tr>
            <th scope="col">#</th>
            <th scope="col">Teacher Name</th>
        </tr>
    </thead>
    <tbody>
        @{
            var data = (List<User>)ViewData["Teachers"];
        }
        @foreach (var teacher in @data)
        {
            <tr>
                <th scope="row">@teacher.Id</th>
                <td>@teacher.FirstName @teacher.LastName</td>
                dsadsa

            </tr>
        }
    </tbody>
</table>

